{
  "name": "restaurant-reviewer",
  "version": "1.0.0",
  "description": "Restaurant Review App",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "production": "webpack -p",
    "lint": "eslint src/.; exit 0",
    "fix": "eslint --fix src/.; exit 0",
    "production:run": "cd dist && http-server",
    "test:karma": "karma start",
    "test": "mocha --require babel-core/register --require ignore-styles ./test/**/**/*.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers babel-core/register --require test/setup.js --recursive 'test/**/*.test.js'",
    "cover:open": "open coverage/lcov-report/index.html"
  },
  "author": "Yuichi Hagio <yhagio87@gmail.com> (http://github.com/yhagio)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.12.0",
    "es6-promise": "^4.0.3",
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^15.3.1",
    "redux-mock-store": "^1.1.4",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
